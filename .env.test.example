# Test Environment Variables Template
# Copy this file to .env.test and fill in your test values
# DO NOT commit .env.test to version control

# Supabase Test Instance
TEST_SUPABASE_URL=http://localhost:54321
TEST_SUPABASE_ANON_KEY=your-test-anon-key-here
TEST_SUPABASE_SERVICE_KEY=your-test-service-key-here

# HuggingFace Test API
TEST_HUGGINGFACE_API_KEY=your-test-hf-key-here

# Redis Test Instance
TEST_REDIS_URL=redis://localhost:6379

# Test Database
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/test_db

# Other Test Variables
TEST_JWT_SECRET=your-test-jwt-secret-here
TEST_ENCRYPTION_KEY=your-test-encryption-key-here