{
  "name": "prisma-portfolio-builder",
  "version": "0.2.0-beta",
  "private": true,
  "description": "PRISMA by MADFAM - AI-powered portfolio builder that transforms CVs into stunning websites in 30 minutes",
  "author": "MADFAM Team",
  "license": "UNLICENSED",
  "keywords": [
    "prisma",
    "portfolio",
    "ai",
    "nextjs",
    "saas",
    "cv",
    "resume",
    "madfam"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aldoruizluna/labspace/ai-portfolio-builder.git"
  },
  "bugs": {
    "url": "https://github.com/aldoruizluna/labspace/ai-portfolio-builder/issues"
  },
  "homepage": "https://prisma.madfam.io",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:clean": "pkill -f 'next dev' || true && pnpm dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --config .eslintrc.strict.js",
    "code:quality": "pnpm lint:strict && pnpm type-check && pnpm test",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "release": "standard-version",
    "supabase:migrate": "supabase db reset",
    "supabase:types": "supabase gen types typescript --local > types/supabase.ts",
    "supabase:reset": "supabase db reset",
    "setup:git": "./scripts/setup-git.sh",
    "docker:dev": "./scripts/docker-dev.sh",
    "docker:prod": "./scripts/docker-prod.sh",
    "check:node": "node scripts/check-node-version.js",
    "docker:dev:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod:up": "docker-compose up -d",
    "docker:prod:down": "docker-compose down",
    "docker:prod:logs": "docker-compose logs -f",
    "seed": "tsx scripts/seed-database.ts",
    "seed:minimal": "tsx scripts/seed-database.ts --mode minimal",
    "seed:demo": "tsx scripts/seed-database.ts --mode demo",
    "seed:full": "tsx scripts/seed-database.ts --mode full",
    "seed:status": "tsx scripts/seed-database.ts --status",
    "seed:reset": "tsx scripts/seed-database.ts --reset"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@octokit/auth-app": "^6.1.3",
    "@octokit/rest": "^20.1.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.80.6",
    "@types/redis": "^4.0.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^0.2.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.18.0",
    "immer": "^10.1.1",
    "isomorphic-dompurify": "^2.25.0",
    "lucide-react": "^0.310.0",
    "next": "15.3.3",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "recharts": "^2.15.3",
    "redis": "^5.5.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.56",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@next/bundle-analyzer": "^15.3.3",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.0",
    "@types/react": "^18.3.23",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.3.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-beautiful-dnd": "^13.1.1",
    "standard-version": "^9.5.0",
    "supabase": "^1.226.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "undici": "^7.10.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "refactor",
          "perf",
          "test",
          "chore",
          "ci",
          "build",
          "revert"
        ]
      ],
      "scope-enum": [
        2,
        "always",
        [
          "auth",
          "editor",
          "ai",
          "db",
          "api",
          "ui",
          "deploy",
          "docs",
          "config",
          "deps",
          "release"
        ]
      ],
      "subject-case": [
        2,
        "never",
        [
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ]
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912"
}
